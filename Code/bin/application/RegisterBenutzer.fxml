<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="467.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.RegisterBenutzerController">
   <children>
      <Label layoutX="85.0" layoutY="6.0" text="Als Benutzer registrieren" textAlignment="JUSTIFY" />
      <TextField fx:id="benutzername" focusTraversable="false" layoutX="76.0" layoutY="50.0" promptText="Benutzername" />
      <TextField fx:id="vorname" focusTraversable="false" layoutX="76.0" layoutY="86.0" promptText="Vorname" />
      <TextField fx:id="name" focusTraversable="false" layoutX="76.0" layoutY="118.0" promptText="Name" />
      <TextField fx:id="land" focusTraversable="false" layoutX="76.0" layoutY="150.0" promptText="Land" />
      <TextField fx:id="stadt" focusTraversable="false" layoutX="76.0" layoutY="184.0" promptText="Stadt" />
      <TextField fx:id="strasse" focusTraversable="false" layoutX="76.0" layoutY="217.0" promptText="Strasse" />
      <TextField fx:id="nummer" focusTraversable="false" layoutX="76.0" layoutY="249.0" promptText="Hausnummer" />
      <TextField fx:id="mail" focusTraversable="false" layoutX="76.0" layoutY="282.0" promptText="Email" />
      <TextField fx:id="passwort" focusTraversable="false" layoutX="76.0" layoutY="314.0" promptText="Passwort" />
      <Button layoutX="63.0" layoutY="356.0" mnemonicParsing="false" onAction="#saveData" text="Registrieren" />
      <Button layoutX="121.0" layoutY="396.0" mnemonicParsing="false" onAction="#SignOut" text="Zurueck" />
      <Label fx:id="nullValidationLabel" layoutX="63.0" layoutY="381.0" prefHeight="27.0" prefWidth="234.0" textAlignment="JUSTIFY" textFill="#f50303" textOverrun="CLIP">
         <font>
            <Font size="18.0" />
         </font></Label>
      <Button layoutX="165.0" layoutY="356.0" mnemonicParsing="false" onAction="#sucheAbbrechen" text="Abbrechen" />
   </children>
</AnchorPane>
